{
    "name": "webbuilders-group/silverstripe-gridfielditemtype",
    "description": "Adds type/class picking functionality to SilverStripe 4's GridField",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "gridfield"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ed Chipman",
            "homepage": "http://webbuildersgroup.com",
            "role": "Developer"
        }
    ],
    "require":
    {
        "php": ">=7.1.0",
        "silverstripe/framework": "~4.1"
    },
    "support": {
        "issues": "https://github.com/webbuilders-group/silverstripe-gridfielditemtype/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        },
        "expose": [
            "css",
            "javascript"
        ]
    },
    "autoload": {
        "psr-4": {
            "WebbuildersGroup\\GridField\\ItemType\\": "src/"
        }
    }
}
