    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "gridfield"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ed Chipman",
            "homepage": "http://webbuildersgroup.com",
            "role": "Developer"
        }
    ],
    "require":
    {
        "silverstripe/framework": "~4.5|~5.0|~6.0"
    },
    "support": {
